			[Enable] [char](1) COLLATE Thai_CI_AS NULL,
			CONSTRAINT [PK_User_1] PRIMARY KEY CLUSTERED
			(
			[UserID] ASC
			)WITH (IGNORE_DUP_KEY = OFF) ON [PRIMARY]
			) ON [PRIMARY]
			GO
			SET ANSI_PADDING OFF
